{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug on Windows",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/exporters/ostream/Debug/ostream_log_test.exe",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false
    },
    {
      "name": "Debug on Linux",
      "type": "gdb",
      "request": "launch",
      "target": "${workspaceFolder}/bazel-bin/<path to the bin file>",
      "cwd": "${workspaceRoot}",
      "valuesFormatting": "parseText"
    }
  ]
}
